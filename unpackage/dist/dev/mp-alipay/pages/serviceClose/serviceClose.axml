<view class="container container-gray"><u-toast vue-id="5829de94-1" ref="__r" data-ref="uToast" onVueInit="__l"></u-toast><form class="container-body" report-submit="true" data-event-opts="{{[['submit',[['applySubmit',['$event']]]]]}}" onSubmit="__e"><view class="group"><view class="group-body"><view class="input-list"><view class="input-item"><button data-event-opts="{{[['tap',[['insertCost',['$event']]]]]}}" class="input-item-lable" style="flex:1;" onTap="__e">增加额外费用</button></view><view class="input-item" style="border-top:0rpx;padding-top:0rpx;"><button data-event-opts="{{[['tap',[['deleteCost',['$event']]]]]}}" class="input-item-lable" style="flex:1;" onTap="__e">删除额外费用</button></view><view class="cost_group" a:for="{{extraCosts}}" a:for-item="item" a:for-index="index" a:key="index"><view class="input-item"><text class="input-item-lable">额外花费</text><picker class="input-item-content" data-pickername="{{'costType'+index}}" value="{{costTypeIndexs[index].mIndex}}" range="{{costTypes}}" mode="selector" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" onChange="__e"><view>{{''+costTypes[costTypeIndexs[index].mIndex]+''}}</view></picker></view><view class="input-item"><text class="input-item-lable">费用</text><view class="input-item-content"><input type="number" placeholder="请输入费用" value=""/></view></view></view><view class="input-item"><text class="input-item-lable">所属项目</text><picker class="input-item-content" value="{{projectIndex}}" range="{{listProject}}" mode="selector" data-event-opts="{{[['change',[['bindPickerProject',['$event']]]]]}}" onChange="__e"><view>{{''+listProject[projectIndex]+''}}</view></picker></view><view class="input-item"><text class="input-item-lable">权重</text><view class="input-item-content"><input type="number" placeholder="请输入权重值" name="weight" disabled="true" value="{{weight}}"/></view></view><view class="input-item"><text class="input-item-lable">服务描述</text><text style="flex:1;text-align:right;" type="text" bindtap="transferDescribe">{{serviceDescribe}}</text><image class="transfer_image" src="../../static/images/transfer.png"></image></view></view></view></view><block a:if="{{popErrorMsg}}"><view class="ad_popError">{{popErrorMsg}}</view></block><view class="btn-submit"><button class="btn-block btn-orange" form-type="submit">提交</button></view></form></view>