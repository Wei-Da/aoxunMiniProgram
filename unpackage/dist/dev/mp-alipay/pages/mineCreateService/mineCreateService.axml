<view class="container container-gray"><form class="container-body" report-submit="true" data-event-opts="{{[['submit',[['applySubmit',['$event']]]]]}}" onSubmit="__e"><view class="group"><view class="group_title"><text class="group_title_text">创建服务</text><view data-event-opts="{{[['tap',[['detailSwitchTap',['$event']]]]]}}" class="switch_detail" onTap="__e"><image class="switch_detail_image" src="{{getImgSrc}}"></image><text class="switch_detail_text">{{isOpenDetail?'隐藏详情':'展开详情'}}</text></view><view class="fun_space"></view><image class="scanner_image" src="../../static/images/scanner.png" data-event-opts="{{[['tap',[['scannerData',['$event']]]]]}}" onTap="__e"></image></view><view hidden="{{!(isOpenDetail)}}" class="group-body"><view class="input-list"><view class="input-item"><text class="input-item-lable">标题</text><view class="input-item-content"><input type="text" placeholder="请输入标题" name="title" value="{{title}}"/></view></view><block a:if="{{!isOtherDevice}}"><view class="input-item"><text class="input-item-lable">公司名称</text><picker class="input-item-content" data-pickername="companyName" value="{{companyNameIndex}}" range="{{companyNames}}" mode="selector" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" onChange="__e"><view class="input-item-content">{{''+companyNames[companyNameIndex]+''}}</view></picker><text data-event-opts="{{[['tap',[['companySearch',['$event']]]]]}}" onTap="__e">公司搜索</text></view></block><view class="input-item"><text class="input-item-lable">服务级别</text><picker class="input-item-content" data-pickername="serviceLevel" value="{{serviceLevelIndex}}" range="{{serviceLevels}}" mode="selector" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" onChange="__e"><view class="input-item-content">{{''+serviceLevels[serviceLevelIndex]+''}}</view></picker></view><view class="input-item"><text class="input-item-lable">创建时间</text><view data-event-opts="{{[['tap',[['toTimeChoose',['$event']]]]]}}" class="input-item-content" onTap="__e"><text>{{createTime}}</text></view></view><view class="input-item"><text class="input-item-lable">超时时间</text><view data-event-opts="{{[['tap',[['toEndTimeChoose',['$event']]]]]}}" class="input-item-content" onTap="__e"><text>{{endTime}}</text></view></view><view class="input-item"><text class="input-item-lable">影响度</text><picker class="input-item-content" data-pickername="influenceLevel" value="{{influenceLevelIndex}}" range="{{influenceLevels}}" mode="selector" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" onChange="__e"><view class="input-item-content">{{''+influenceLevels[influenceLevelIndex]+''}}</view></picker></view><view class="input-item"><text class="input-item-lable">设备所属</text><block a:if="{{!isOtherDevice}}"><picker class="input-item-content" value="{{deviceDepartmentIndex}}" range="{{deviceDepartments}}" mode="selector" data-pickername="deviceDepartment" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" onChange="__e"><view class="input-item-content">{{''+deviceDepartments[deviceDepartmentIndex]+''}}</view></picker></block><block a:if="{{isOtherDevice}}"><text style="flex:1;" type="text">其它</text></block><text data-event-opts="{{[['tap',[['changeDeviceFrom',['$event']]]]]}}" class="input-item-change" onTap="__e">{{deviceType}}</text></view><view class="input-item"><text class="input-item-lable">设备名称</text><block a:if="{{!isOtherDevice}}"><picker class="input-item-content" value="{{deviceNameIndex}}" range="{{deviceNames}}" mode="selector" data-pickername="deviceName" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" onChange="__e"><view class="input-item-content">{{''+deviceNames[deviceNameIndex]+''}}</view></picker></block><block a:if="{{!isOtherDevice}}"><text data-event-opts="{{[['tap',[['deviceSearch',['$event']]]]]}}" onTap="__e">设备搜索</text></block><block a:if="{{isOtherDevice}}"><input style="flex:1;" type="text" placeholder="请输入设备名称" name="otherDeviceName" value="{{otherDeviceName}}"/></block></view><block a:if="{{!isOtherDevice}}"><view class="input-item"><text class="input-item-lable">负责人</text><view class="input-item-content"><input type="text" placeholder="请输入负责人" name="serviceIp" value="{{devicePerson}}"/></view></view></block><block a:if="{{!isOtherDevice}}"><view class="input-item"><text class="input-item-lable">部门</text><view class="input-item-content"><input type="text" placeholder="请输入部门" name="serviceIp" value="{{personDepartment}}"/></view></view></block><block a:if="{{!isOtherDevice}}"><view class="input-item"><text class="input-item-lable">联系电话</text><view class="input-item-content"><input type="text" placeholder="请输入联系电话" name="serviceIp" value="{{personPhone}}"/></view></view></block></view></view><view class="{{(isOpenDetail?'create_des':'group-body')}}"><view class="input-list"><view class="input-item"><text class="input-item-lable">服务描述</text><text style="flex:1;text-align:right;" type="text" data-event-opts="{{[['tap',[['transferDescribe',['$event']]]]]}}" onTap="__e">{{serviceDescribe}}</text><image class="transfer_image" src="../../static/images/transfer.png"></image></view><view class="input-item"><text class="input-item-lable">语音描述</text><block a:if="{{audioPath!=''}}"><view data-event-opts="{{[['tap',[['bindtapPlayAudio',['$event']]]]]}}" class="audio_record_group" onTap="__e"><image class="audio_image" src="../../static/images/audio_icon.png"></image><text>语音</text><image class="record_image" src="{{getRecordImg}}"></image></view></block><text style="flex:1;text-align:right;" type="text"></text><view data-event-opts="{{[['touchStart',[['bindtouchstart',['$event']]]],['touchEnd',[['bindtouchend',['$event']]]]]}}" class="{{(isReady?'record_group':'start_record')}}" onTouchStart="__e" onTouchEnd="__e"><text class="{{(isReady?'record_text':'record_start_text')}}">{{isReady?'长按录音':'松开结束'}}</text><image class="record_image" src="{{getRecordImg}}"></image></view></view></view></view><view class="group-header">上传图片</view><view class="group-body"><view class="img-upload"><view class="gallery"><view class="item" a:for="{{uploadimgs}}" a:for-item="item" a:for-index="index" a:key="index"><image style="{{'width:'+(imageWidth+'px')+';'+('height:'+(imageWidth+'px')+';')}}" src="{{item}}" mode="aspectFill" data-event-opts="{{[['tap',[['previewImage',['$event']]]]]}}" onTap="__e"></image><view class="delete" data-index="{{index}}" data-event-opts="{{[['tap',[['deleteImage',['$event']]]]]}}" onTap="__e"><image style="{{'left:'+(imageWidth/2-10+'px')+';'}}" src="../../static/images/icon_delete.png"></image></view></view><view class="item"><image class="button-upload" style="{{'width:'+(imageWidth+'px')+';'+('height:'+(imageWidth+'px')+';')}}" src="../../static/images/icon_add.png" data-event-opts="{{[['tap',[['chooseImage',['$event']]]]]}}" onTap="__e"></image></view></view></view></view></view><block a:if="{{popErrorMsg}}"><view class="ad_popError">{{popErrorMsg}}</view></block><view class="btn-submit"><button class="btn-block btn-orange" form-type="submit">提交</button></view></form></view>