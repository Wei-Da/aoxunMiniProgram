
.product-company {
	width: 100%;
	height: 30rpx;
	text-align: center;
	line-height: 30rpx;
	font-size: 20rpx;
}
.logo-content {
	width: 100%;
	height: 300rpx;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.logo-content-detail {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	width: 160rpx;
	margin-top: 130rpx;
}
.logo-content-detail-image {
	width: 110rpx;
	height: 80.88rpx;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	margin-left: 25rpx;
	margin-right: 25rpx;
}
.logo-content-detail-text {
	height: 50rpx;
	line-height: 50rpx;
	margin-left: 10rpx;
	text-align: center;
}
.input-account {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	height: 80rpx;
	margin: 20rpx 60rpx;
}
.account-name {
	height: 67rpx;
	line-height: 67rpx;
	text-align: center;
	margin-right: 15rpx;
}
.account-content {
	height: 65rpx;
	width: 100%;
	border-bottom: 1rpx solid gainsboro;
	font-size: 25rpx;
}
.account-checkbox {
	height: 20rpx;
	width: 300rpx;
}
.account-image {
	height: 46rpx;
	width: 50rpx;
	margin: 9.5rpx 0;
	margin-right: 15rpx;
}
.tip-text {
	text-align: center;
	padding: 20rpx;
	color: red;
}
.radio-group {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
.radio-item {
	position: static;
}
.copyright {
	position: fixed;
	bottom: 0;
	width: 100%;
}

