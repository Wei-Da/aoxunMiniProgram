<view class="all_group"><view class="title_group"><view class="title_group_search"><image class="search_image" src="../../static/images/search_img.png"></image><input class="search_input" placeholder="请输入关键词" data-event-opts="{{[['input',[['onInputListener',['$event']]]]]}}" value="{{keyWords}}" onInput="__e"/><image hidden="{{!(deleteHidden)}}" class="delete_image" src="../../static/images/search_edit_delete.png" data-event-opts="{{[['tap',[['clearInputData',['$event']]]]]}}" onTap="__e"></image></view><text data-event-opts="{{[['tap',[['keywordSearch',['$event']]]]]}}" class="title_cancel" onTap="__e">搜索</text></view><block a:if="{{!searchHiddlen&&hasRecord}}"><view class="group-detail"><view class="record_history_group"><text class="record_history_text">常搜公司:</text><view class="record_content_group"><block a:for="{{recordHistory}}" a:for-item="item" a:for-index="index"><text class="record_content" data-pickername="{{item}}" data-event-opts="{{[['tap',[['chooseKeyword',['$event']]]]]}}" onTap="__e">{{item}}</text></block></view></view></view></block><block a:if="{{!searchHiddlen}}"><view class="group-detail"><text class="record_history_text" style="background:#F0EFF4;">公司名称:</text><block a:for="{{deviceList}}" a:for-item="item" a:for-index="index"><text class="device_name" data-index="{{index}}" data-event-opts="{{[['tap',[['chooseDevice',['$event']]]]]}}" onTap="__e">{{item}}</text></block></view></block><block a:if="{{searchHiddlen}}"><view class="group-detail"><block a:for="{{searchDeviceList}}" a:for-item="item" a:for-index="index"><text class="device_name" data-index="{{index}}" data-event-opts="{{[['tap',[['chooseSerachDevice',['$event']]]]]}}" onTap="__e">{{item.name}}</text></block></view></block></view>