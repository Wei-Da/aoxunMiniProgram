<view class="container"><view class="container-body"><view class="date-view-dis">{{year+"-"+month+"-"+day+" "+hour+":"+minute+":"+second}}</view><view class="date-view"><picker-view style="width:100%;height:200rpx;" indicator-style="height: 80rpx;" value="{{value}}" data-event-opts="{{[['change',[['bindChange',['$event']]]]]}}" onChange="__e"><picker-view-column><view style="line-height:80rpx;" a:for="{{dateTimeArray[0]}}" a:for-item="item" a:for-index="index" a:key="index">{{item}}</view></picker-view-column><picker-view-column><view style="line-height:80rpx;" a:for="{{dateTimeArray[1]}}" a:for-item="item" a:for-index="index" a:key="index">{{item}}</view></picker-view-column><picker-view-column><view style="line-height:80rpx;" a:for="{{monthDay}}" a:for-item="item" a:for-index="index" a:key="index">{{item}}</view></picker-view-column></picker-view></view><view class="date-view"><picker-view style="width:100%;height:200rpx;" indicator-style="height: 80rpx;" value="{{valueHour}}" data-event-opts="{{[['change',[['bindChangeHour',['$event']]]]]}}" onChange="__e"><picker-view-column><view style="line-height:80rpx;" a:for="{{dateTimeArray[3]}}" a:for-item="item" a:for-index="index" a:key="index">{{item}}</view></picker-view-column><picker-view-column><view style="line-height:80rpx;" a:for="{{dateTimeArray[4]}}" a:for-item="item" a:for-index="index" a:key="index">{{item}}</view></picker-view-column><picker-view-column><view style="line-height:80rpx;" a:for="{{dateTimeArray[5]}}" a:for-item="item" a:for-index="index" a:key="index">{{item}}</view></picker-view-column></picker-view></view></view><view class="btn-confirm"><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="btn-block btn-orange" onTap="__e">确定</button></view></view>