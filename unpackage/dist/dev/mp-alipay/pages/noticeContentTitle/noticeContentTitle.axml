<view><scroll-view class="container-body" scroll-y="{{true}}" scroll-top="{{scrolltop}}" lower-threshold="50" data-event-opts="{{[['scrollToLower',[['scrollLoading',['$event']]]],['scrollToUpper',[['onPullDownRefresh',['$event']]]]]}}" onScrollToLower="__e" onScrollToUpper="__e"><view class="info_group"><block a:if="{{serviceList.length==0&&isFinishRequest}}"><view class="no_data_view"><image src="../../static/images/no_data.png"></image><text class="no_data_text">没有数据</text></view></block><block a:for="{{serviceList}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{serviceList.length!=0}}"><navigator class="info_item" data-index="{{index}}" data-event-opts="{{[['tap',[['bindtapTurnto',['$event']]]]]}}" onTap="__e"><view style="display:flex;flex:1;"><text class="info_text">{{item.noticeTitle}}</text></view><image class="info_image_transfer" src="../../static/images/transfer.png"></image></navigator></block></block></view></scroll-view></view>