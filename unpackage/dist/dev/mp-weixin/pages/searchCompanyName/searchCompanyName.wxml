<view class="all_group"><view class="title_group"><view class="title_group_search"><image class="search_image" src="../../static/images/search_img.png"></image><input class="search_input" placeholder="请输入关键词" data-event-opts="{{[['input',[['onInputListener',['$event']]]]]}}" value="{{keyWords}}" bindinput="__e"/><image hidden="{{!(deleteHidden)}}" class="delete_image" src="../../static/images/search_edit_delete.png" data-event-opts="{{[['tap',[['clearInputData',['$event']]]]]}}" bindtap="__e"></image></view><text data-event-opts="{{[['tap',[['keywordSearch',['$event']]]]]}}" class="title_cancel" bindtap="__e">搜索</text></view><block wx:if="{{!searchHiddlen&&hasRecord}}"><view class="group-detail"><view class="record_history_group"><text class="record_history_text">常搜公司:</text><view class="record_content_group"><block wx:for="{{recordHistory}}" wx:for-item="item" wx:for-index="index"><block><text class="record_content" data-pickername="{{item}}" data-event-opts="{{[['tap',[['chooseKeyword',['$event']]]]]}}" bindtap="__e">{{item}}</text></block></block></view></view></view></block><block wx:if="{{!searchHiddlen}}"><view class="group-detail"><text class="record_history_text" style="background:#F0EFF4;">公司名称:</text><block wx:for="{{deviceList}}" wx:for-item="item" wx:for-index="index"><block><text class="device_name" data-index="{{index}}" data-event-opts="{{[['tap',[['chooseDevice',['$event']]]]]}}" bindtap="__e">{{item}}</text></block></block></view></block><block wx:if="{{searchHiddlen}}"><view class="group-detail"><block wx:for="{{searchDeviceList}}" wx:for-item="item" wx:for-index="index"><block><text class="device_name" data-index="{{index}}" data-event-opts="{{[['tap',[['chooseSerachDevice',['$event']]]]]}}" bindtap="__e">{{item.name}}</text></block></block></view></block></view>